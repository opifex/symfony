{% extends '@!NelmioApiDoc/SwaggerUi/index.html.twig' %}

{% block meta %}
    {{ parent() }}
{% endblock meta %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        body {
            margin: 10px 0;
            background: rgb(255, 255, 255);
        }

        i, cite, em, var, address, dfn {
            font-style: normal;
        }

        header,
        .swagger-ui .servers-title,
        .swagger-ui .info .description,
        .swagger-ui .info .title .version-stamp {
            display: none;
        }

        .swagger-ui .servers>label {
            margin: -20px 20px 0 0;
        }

        #swagger-ui.api-platform .info {
            padding: 0 60px;
        }

        #swagger-ui.api-platform select {
            max-width: 340px;
            width: 100%;
            padding: 8px 10px;
        }

        #swagger-ui.api-platform .info .title {
            color: rgb(59, 65, 81);
        }

        #swagger-ui.api-platform .opblock {
            box-shadow: 0 0 3px rgb(0, 0, 0, 0.2);
            padding: 5px;
        }

        #swagger-ui.api-platform .opblock-tag-section h3 {
            border-bottom: 1px solid rgb(0, 0, 0, 0.1);
        }

        #swagger-ui.api-platform .opblock-tag-section.is-open h3 {
            border-bottom: 1px solid rgb(255, 255, 255);
        }

        #swagger-ui.api-platform .opblock-tag {
            margin: 0;
            padding: 10px 10px;
        }

        #swagger-ui.api-platform .opblock-tag:hover {
            background-color: rgba(255, 255, 255);
            transform: none;
        }

        #swagger-ui.api-platform .opblock-summary {
            border: 0;
        }

        #swagger-ui.api-platform .opblock-summary-method {
            width: 90px;
        }

        #swagger-ui.api-platform .btn:hover {
            border-color: rgb(128, 128, 128);
            background-color: rgba(65, 68, 78, 0.14);
        }

        #swagger-ui.api-platform .btn.authorize:hover {
            border-color: rgb(73, 204, 144);
            background-color: rgb(120, 188, 97, 0.19);
        }

        #swagger-ui.api-platform .information-container.wrapper {
            margin: 0;
            border-bottom: 0;
        }

        .swagger-ui .servers {
            margin-bottom: 3px;
        }

        .swagger-ui .scheme-container {
            padding: 0;
            box-shadow: none;
            margin: 20px 0 10px 0;
        }

        .swagger-ui .models {
            display: none;
        }

        .swagger-ui .info > div {
            margin: 0;
        }

        .swagger-ui .info .title small {
            background: rgb(104, 157, 86);
        }

        .swagger-ui .info hgroup.main {
            margin: 0 0 10px;
        }

        .swagger-ui .opblock-summary {
            border: 0;
        }

        .swagger-ui .opblock-summary .arrow {
            margin-right: 11px;
        }

        .swagger-ui .opblock-summary .authorization__btn {
            margin-right: 1px;
            padding-left: 0;
        }

        .swagger-ui .opblock-body pre.microlight {
            padding: 1.0em;
            font-weight: normal;
        }

        .swagger-ui .response p,
        .swagger-ui .parameters p {
            margin-top: 0;
        }

        .swagger-ui section.models {
            border: 0;
            border-radius: 0;
            box-shadow: 0 0 3px rgb(0, 0, 0, 0.2);
            margin: 30px 0;
        }

        .swagger-ui section.models h4 {
            padding: 10px 15px 10px 10px;
        }

        .swagger-ui section.models h4:hover {
            background: rgb(255, 255, 255);
        }

        .swagger-ui section.models h4 span {
            font-size: 24px;
            color: rgb(59, 65, 81);
        }

        .swagger-ui section.models.is-open {
            padding: 0 0 1px 0;
        }

        .swagger-ui section.models.is-open h4 {
            border-bottom: 0;
            margin: 0;
        }

        .swagger-ui .model-box {
            padding: 10px 20px 15px 20px;
            line-height: 100%;
        }

        .swagger-ui .model-box .model {
            margin-top: 10px;
        }

        .swagger-ui .model-box .model-box {
            padding: 0;
        }

        .swagger-ui .model-box .property-row td {
            padding-bottom: 10px;
        }

        .swagger-ui table.headers .header-example,
        .swagger-ui .model-box .property.primitive,
        .swagger-ui .parameter__extension, .swagger-ui .parameter__in {
            font-style: normal;
            font-weight: normal;
            color: rgb(170, 170, 170);
        }

        .swagger-ui .auth-container input[type=text],
        .swagger-ui .auth-container input[type=password] {
            width: 100%;
        }

        .swagger-ui .auth-container h4 {
            text-align: center;
        }

        .swagger-ui .opblock-body {
            margin-top: 5px;
        }

        .swagger-ui .opblock-body div.parameter__enum {
            margin-bottom: 5px;
        }

        .swagger-ui .headers__title {
            display: none;
        }

        .swagger-ui .response-control-media-type__accept-message {
            font-size: 0.8em;
        }

        .swagger-ui .opblock-body,
        .swagger-ui .responses-inner h4,
        .swagger-ui .responses-inner h5,
        .swagger-ui .parameter__name,
        .swagger-ui table thead tr td,
        .swagger-ui table thead tr th,
        .swagger-ui .auth-container label {
            font-size: 14px;
        }

        .swagger-ui .response-col_links {
            font-style: normal;
        }

        .swagger-ui section.models .model-container,
        .swagger-ui section.models .model-container:first-of-type,
        .swagger-ui section.models .model-container:last-of-type {
            border-radius: 0;
            margin: 0 5px 15px;
        }

        #swagger-ui.api-platform .opblock.opblock-get .opblock-summary-method,
        .swagger-ui .opblock.opblock-get .tab-header .tab-item.active h4 span:after {
            background: rgb(52, 114, 176);
        }

        #swagger-ui.api-platform .opblock.opblock-get .opblock-section-header {
            background: rgb(52, 114, 176, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-put .opblock-summary-method,
        .swagger-ui .opblock.opblock-put .tab-header .tab-item.active h4 span:after {
            background: rgb(222, 195, 81);
        }

        #swagger-ui.api-platform .opblock.opblock-put .opblock-section-header {
            background: rgb(222, 195, 81, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-post .opblock-summary-method,
        .swagger-ui .opblock.opblock-post .tab-header .tab-item.active h4 span:after {
            background: rgb(104, 157, 86);
        }

        #swagger-ui.api-platform .opblock.opblock-post .opblock-section-header {
            background: rgb(104, 157, 86, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-delete .opblock-summary-method,
        .swagger-ui .opblock.opblock-delete .tab-header .tab-item.active h4 span:after {
            background: rgb(224, 81, 64);
        }

        #swagger-ui.api-platform .opblock.opblock-delete .opblock-section-header {
            background: rgb(224, 81, 64, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-options .opblock-summary-method,
        .swagger-ui .opblock.opblock-options .tab-header .tab-item.active h4 span:after {
            background: rgb(72, 175, 185);
        }

        #swagger-ui.api-platform .opblock.opblock-options .opblock-section-header {
            background: rgb(72, 175, 185, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-head .opblock-summary-method,
        .swagger-ui .opblock.opblock-head .tab-header .tab-item.active h4 span:after {
            background: rgb(133, 84, 175);
        }

        #swagger-ui.api-platform .opblock.opblock-head .opblock-section-header {
            background: rgb(133, 84, 175, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-patch .opblock-summary-method,
        .swagger-ui .opblock.opblock-patch .tab-header .tab-item.active h4 span:after {
            background: rgb(96, 161, 146);
        }

        #swagger-ui.api-platform .opblock.opblock-patch .opblock-section-header {
            background: rgb(96, 161, 146, 0.1);
        }

        #swagger-ui.api-platform .opblock.opblock-trace .opblock-summary-method,
        .swagger-ui .opblock.opblock-trace .tab-header .tab-item.active h4 span:after {
            background: rgb(54, 65, 70);
        }

        #swagger-ui.api-platform .opblock.opblock-trace .opblock-section-header {
            background: rgb(54, 65, 70, 0.1);
        }
    </style>
{% endblock stylesheets %}

{% block header %}

{% endblock header %}
